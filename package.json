{
  "name": "image-editor",
  "version": "1.0.0",
  "description": "a webapp for editing images",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "sass webapp/scss:webapp/css && webpack --mode=production --node-env=production",
    "build:dev": "sass webapp/scss:webapp/css && webpack --mode=development",
    "build:prod": "sass webapp/scss:webapp/css && webpack --mode=production --node-env=production",
    "watch": "sass webapp/scss:webapp/css && webpack --watch",
    "serve": "sass webapp/scss:webapp/css && webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tniyer2/image-editor-nodejs.git"
  },
  "author": "Tanishq Iyer",
  "bugs": {
    "url": "https://github.com/tniyer2/image-editor-nodejs/issues"
  },
  "homepage": "https://github.com/tniyer2/image-editor-nodejs#readme",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@webpack-cli/generators": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^5.16.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "chroma": "^0.0.1"
  }
}
